{
  "key": "${addon.key}",
  "baseUrl": "https://b0e5add9.ngrok.io",
  "name": "Jira Scrum Agent",
  "vendor": {
    "name": "Lymantridae, Inc."
  },
  "authentication": {
    "type": "jwt"
  },
  "lifecycle": {
    "installed": "/installed",
    "uninstalled": "/uninstalled"
  },
  "scopes" : [
      "READ",
      "WRITE",
      "DELETE",
      "ADMIN"
    ]
  ,
  "apiMigrations": {
    "gdpr": true
  },
  "modules": {
    "generalPages": [
      {
        "url": "/home",
        "key": "home",
        "location": "system.top.navigation.bar",
        "name": {
          "value": "JiraScrumAgent"
        }
      }
    ],
    "webhooks": [
      {
        "event": "host_settings_saved",
